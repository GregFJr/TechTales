{{> navbar }}

<body>
    <h1 class="dash-header">{{userId}}Your Dashboard</h1>

    <a href="/dashboard/add-post" class="add-post">Add New Post</a>

    <div>
        {{#each posts}}
        <div class="post">
            <h2>{{this.title}}</h2>
            <p>{{this.content}}</p>
            <a href="/edit-post/{{this.id}}" class="edit-post">Edit</a> | <a href="#" class="delete-post" data-id="{{this.id}}">Delete</a>

        </div>
        {{/each}}
    </div>
</body>
<script src="/js/script.js"></script>